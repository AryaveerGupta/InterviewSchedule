<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>

<body>
    <form action="/create" , enctype="application/x-www-form-urlencoded" , method="post">
        <label for="st">Start Time</label>
        <input type="datetime-local" id="st" name="start" placeholder="Give the start datetime" required>

        <label for="en">End Time</label>
        <input type="datetime-local" id="en" name="end" placeholder="Give the end datetime" required>

        <label for="par">Participants</label>
        <%  users.forEach(user => {  %>
            <label for="<%= user.Name%>"><%= user.Name%></label>
            <input type="checkbox" name="participants" id="<%= user.Name%>" value="<%= user.id%>">
            <br>
        <%  }) %>   
            <input type="submit" value="Submit">
    </form>
</body> 

</html>